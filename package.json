{
    "name": "squad-timer",
    "version": "0.0.0",
    "scripts": {
        "build:env:pre": "ts-node --transpile-only --project build/tsconfig.json build/generate-environment.ts",
        "build:production": "run-s build:*:pre && ng build --configuration production && run-s build:*:post",
        "build:nginx:post": "ts-node --transpile-only --project build/tsconfig.json build/nginx.conf.ts --configuration production > dist/nginx.conf",
        "compile:ts": "tsc -p tsconfig.strict.json",
        "heroku-postbuild": "yarn run build:production && mkdir config && mv dist/nginx.conf config/nginx.conf.erb",
        "lint": "npm-run-all -cp \"lint:css {@}\" \"lint:ts {@}\" -- ",
        "lint:css": "stylelint \"src/**/*.scss\" --syntax scss",
        "lint:ts": "eslint \"src/**/*.ts\" \"build/**/*.ts\"",
        "ng": "ng",
        "start": "ng serve",
        "test": "yarn run compile:ts"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "~0.1102.11",
        "@angular/animations": "^11.2.12",
        "@angular/cdk": "11.2.11",
        "@angular/cli": "^11.2.11",
        "@angular/common": "^11.2.12",
        "@angular/compiler": "^11.2.12",
        "@angular/compiler-cli": "^11.2.12",
        "@angular/core": "^11.2.12",
        "@angular/forms": "^11.2.12",
        "@angular/language-service": "^11.2.12",
        "@angular/material": "^11.2.11",
        "@angular/platform-browser": "^11.2.12",
        "@angular/platform-browser-dynamic": "^11.2.12",
        "@angular/router": "^11.2.12",
        "@angular/service-worker": "^11.2.12",
        "@sentry/browser": "^5.19.1",
        "@sentry/integrations": "^5.19.1",
        "@types/argparse": "^1.0.0",
        "@types/common-tags": "^1.8.0",
        "@types/node": "^12.11.1",
        "angular-error-repository": "^2.0.0",
        "angular-svg-icon": "^11.2.0",
        "argparse": "^1.0.0",
        "common-tags": "^1.8.0",
        "date-fns": "^2.16.1",
        "ngx-toastr": "^13.2.1",
        "npm-run-all": "^4.1.5",
        "rxjs": "^6.6.3",
        "ts-node": "~7.0.0",
        "tslib": "^2.0.0",
        "typescript": "4.1.5",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@matthiaskunnen/eslint-config-angular": "^3.0.0",
        "codelyzer": "^6.0.0",
        "husky": "^1.3.1",
        "lint-staged": "^10.2.2",
        "npm-run-all": "^4.1.5",
        "stylelint": "^13.6.1",
        "stylelint-order": "^3.1.1"
    },
    "resolutions": {
        "localforage": "^1.9.0"
    },
    "engines": {
        "node": "^12.0.0",
        "yarn": "~1.22.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
